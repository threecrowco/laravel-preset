{
    "name": "threecrowco/laravel-preset",
    "description": "Tailwind CSS Frontend Preset for Laravel 6",
    "keywords": ["laravel", "preset"],
    "license": "MIT",
    "require": {
        "laravel/framework": "^6.0"
    },
    "minimum-stablity": "dev",
    "autoload": {
        "psr-4": {
            "ThreeCrowCo\\LaravelPreset\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ThreeCrowCo\\LaravelPreset\\PresetServiceProvider"
            ]
        }
    }
}
